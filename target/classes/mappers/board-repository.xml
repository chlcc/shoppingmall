<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.sup2is.mapper.BoardRepository">

	<insert id="write" parameterType="org.sup2is.form.BoardForm">
		INSERT INTO board (username,password,category,title,content)
		VALUES (#{username},#{password},#{category},#{title},#{content})
	</insert>
</mapper>